train_net: "dvia_pretrain.prototxt"
test_net: "dvia_pretest.prototxt"

test_iter: 100
test_interval: 1000

base_lr: 0.0001
momentum: 0.0
weight_decay: 0.001

lr_policy: "inv"
gamma: 0.0001
power: 0.75

display: 100

max_iter: 200000

snapshot: 25000
snapshot_prefix: "cifar_pretrain"
solver_mode: GPU

type: "RMSProp"
rms_decay: 0.98
